<?xml version="1.0" encoding="utf-8"?>
<manifest package="io.moku.davide.sideproject"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".myFriends.FriendsListActivity"
            android:label="@string/my_friends" />
        <activity
            android:name=".profile.ProfileActivity"
            android:label="@string/profile" />
        <activity android:name=".profile.UserInformationActivity"
            android:label="@string/info"/>
        <activity android:name=".utils.activity.BasicActivity" />
        <activity android:name=".utils.activity.BasicSecondaryActivity" />
    </application>

</manifest>